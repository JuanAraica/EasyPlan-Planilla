@model IEnumerable<EasyPlan.Models.Tbl_Historial>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDemo.cshtml";
}

 

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Registro)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Registro)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.idRegistro }) |
                @Html.ActionLink("Details", "Details", new { id = item.idRegistro }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.idRegistro })
            </td>
        </tr>
    }

</table>
